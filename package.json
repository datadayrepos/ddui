{
  "name": "@datadayrepos/ddui",
  "type": "module",
  "version": "0.0.1",
  "private": true,
  "packageManager": "pnpm@8.9.0",
  "description": "Uix library",
  "author": "datadayrepos",
  "license": "Apache 2.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/datadayrepos/ddui.git"
  },
  "bugs": {
    "url": "https://github.com/datadayrepos/ddui/issues"
  },
  "keywords": [
    "uix",
    "dataday",
    "vue"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/types/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "lint": "eslint --cache .",
    "lint:fix": "eslint . --fix",
    "release": "bumpp -r && pnpm -r publish",
    "test": "echo \"Error: no test specified\" && exit 1",
    "typecheck": "tsc --noEmit",
    "build": "tsc",
    "pub": "npm publish --access public"
  },
  "peerDependencies": {
    "@datadayrepos/usevuecore": "10.5.0-32",
    "@datadayrepos/usevueshared": "10.5.0-32",
    "vue": "3.3.4",
    "vue-router": "4.2.5"
  },
  "dependencies": {
    "@datadayrepos/asyncvalidator": "0.0.1-beta.1",
    "@datadayrepos/lodashts": "0.0.1-beta.35",
    "@datadayrepos/popperts": "0.0.1-beta.21",
    "@datadayrepos/tinycolor": "0.0.1-beta.1",
    "@element-plus/icons-vue": "2.1.0",
    "@floating-ui/dom": "^1.0.1",
    "dayjs": "1.11.10",
    "memoize-one": "^6.0.0"
  },
  "devDependencies": {
    "@datadayrepos/eslint-config": "^1.0.1-beta.11",
    "@datadayrepos/eslint-plugin-datadayrepos": "1.0.0-Beta12",
    "@datadayrepos/ni": "^0.21.8",
    "@types/node": "20.9.0",
    "@vitejs/plugin-vue": "^4.4.0",
    "@vitejs/plugin-vue-jsx": "^3.0.2",
    "eslint": "^8.51.0",
    "eslint-plugin-sort-keys": "^2.3.5",
    "lint-staged": "^14.0.1",
    "pnpm": "^8.8.0",
    "postcss": "^8.4.5",
    "postcss-import": "^14.0.2",
    "typescript": "^5.2.2",
    "unplugin-vue-macros": "^2.6.2",
    "vue-tsc": "^1.8.19"
  },
  "pnpm": {
    "peerDependencyRules": {
      "ignoreMissing": [
        "vite",
        "react",
        "react-dom"
      ]
    }
  },
  "lint-staged": {
    "*.{js,ts,tsx,vue,md}": [
      "eslint --cache --fix"
    ]
  }
}
